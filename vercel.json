{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/notes.mjs": {
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}